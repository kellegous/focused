{
  "name" : "__MSG_appName__",
  "version" : "0.5",
  "manifest_version" : 2,
  "description" : "__MSG_appDesc__",
  "default_locale": "en",
  "content_scripts" : [
    {
      "matches" : [ "http://*/*", "https://*/*" ],
      "js" : ["js/cs.js", "js/jquery.min.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [ "js/bg.js" ]
  },
  "web_accessible_resources": [
    "im/bg.png",
    "im/lock-lo.png",
    "im/lock-hi.png",
    "im/sig.png",
    "ss/cs.css"
  ],
  "browser_action": {
    "default_icon": {
      "19": "im/ba-lo.png",
      "38": "im/ba-lo.png"
    },
    "default_title": "__MSG_appName__"
  },
  "icons": {
    "128": "im/128.png",
    "64": "im/064.png",
    "32": "im/032.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}