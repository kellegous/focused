{
  "name" : "Clear",
  "version" : "0.1",
  "manifest_version" : 2,
  "description" : "Clear",
  "content_scripts" : [
    {
      "matches" : [ "http://*/*", "https://*/*" ],
      "js" : ["js/cs.js", "js/jquery.min.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [ "js/bg.js" ]
  },
  "web_accessible_resources": [
    "im/bg.png",
    "im/lock-lo.png",
    "im/lock-hi.png",
    "ss/cs.css"
  ],
  "permissions": [
    "tabs"
  ]
}