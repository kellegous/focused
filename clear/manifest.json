{
  "name" : "Focused",
  "version" : "0.1",
  "manifest_version" : 2,
  "description" : "Focused",
  "content_scripts" : [
    {
      "matches" : [ "http://*/*", "https://*/*" ],
      "js" : ["js/cs.js", "js/jquery.min.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [ "js/bg.js" ]
  },
  "web_accessible_resources": [
    "im/bg.png",
    "im/lock-lo.png",
    "im/lock-hi.png",
    "im/sig.png",
    "ss/cs.css"
  ],
  "browser_action": {
    "default_icon": {
      "19": "im/ba-lo.png",
      "38": "im/ba-lo.png"
    },
    "default_title": "Focused"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}